{
    "extends": "standard",
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "mocha": true,
        "jquery": true
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "semi": ["error", "always"],
        "no-var": ["error"],
        "curly": ["error", "all"],
        "no-useless-computed-key": ["error"],
        "dot-notation": ["error"]
    }
}